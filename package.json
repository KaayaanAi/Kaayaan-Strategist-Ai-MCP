{
  "name": "mcp-kaayaan-strategist",
  "version": "2.1.0",
  "description": "Universal MCP server with quad-protocol support (STDIO MCP, HTTP REST, HTTP MCP, WebSocket MCP) for professional market analysis and systematic trading",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "kaayaan-strategist": "./build/index.js"
  },
  "files": [
    "build/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    ".env.example",
    ".env.production.example"
  ],
  "scripts": {
    "build": "tsc && chmod 755 build/index.js",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js",
    "prepublishOnly": "npm run build",
    "start": "node build/index.js",
    "start:stdio": "node build/index.js",
    "start:http": "HTTP_MODE=true node build/index.js",
    "dev": "npm run build && npm run start",
    "dev:http": "npm run build && npm run start:http",
    "test": "node --test tests/*.test.js",
    "test:http": "HTTP_MODE=true npm test"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "trading",
    "market-analysis",
    "technical-indicators",
    "finance",
    "claude",
    "ai",
    "rsi",
    "macd",
    "yahoo-finance",
    "coingecko",
    "cryptocurrency",
    "bitcoin",
    "ethereum",
    "crypto-analysis",
    "multi-asset",
    "mongodb",
    "redis",
    "kuwait",
    "http-api",
    "rest-api",
    "mcp-protocol",
    "n8n",
    "express",
    "stdio",
    "websocket",
    "universal-mcp",
    "quad-protocol",
    "financial-analysis",
    "stock-analysis",
    "crypto-trading",
    "trading-signals",
    "support-resistance",
    "moving-averages",
    "volatility-analysis",
    "typescript",
    "docker",
    "educational",
    "systematic-trading"
  ],
  "author": "Kaayaan Ai <admin@kaayaan.ai>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@types/ws": "^8.18.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mongodb": "^6.3.0",
    "redis": "^4.6.12",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaayaan/mcp-kaayaan-strategist.git"
  },
  "homepage": "https://github.com/kaayaan/mcp-kaayaan-strategist#readme",
  "bugs": {
    "url": "https://github.com/kaayaan/mcp-kaayaan-strategist/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/kaayaan"
  },
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ]
}
